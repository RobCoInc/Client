<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="suplex.theblacklist">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".admin.AdminActivity" />
        <activity android:name=".admin.AdminMainActivity" />
        <activity android:name=".admin.UserListActivity" />
        <activity android:name=".navDrawer.DrawerBaseActivity" />
        <activity android:name=".admin.NewLocationActivity" />
        <activity android:name=".admin.LocationListActivity" />
        <activity android:name=".admin.LocationActivity" />
        <activity android:name=".user.UserMainActivity" />
        <activity android:name=".login.SuccessfulLoginActivity"></activity>
    </application>

</manifest>